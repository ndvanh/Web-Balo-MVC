@model IEnumerable<WebbanBALO.Areas.Admin.Models.Framework.Themanh>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}

<h2>Kho Ảnh</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Upanh)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            <img src="@Url.Content(item.Upanh)" width="100" height="100" />
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Maanh }) |
            @Html.ActionLink("Details", "Details", new { id=item.Maanh }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Maanh })
        </td>
    </tr>
}

</table>
