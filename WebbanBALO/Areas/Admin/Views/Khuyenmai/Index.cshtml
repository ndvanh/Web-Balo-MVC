@model IEnumerable<WebbanBALO.Areas.Admin.Models.Framework.Khuyenmai>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}

<h2>Thông Tin Khuyến Mãi</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Chitietkm)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Anhkm)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Chitietkm)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Anhkm)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Makm }) |
            @Html.ActionLink("Details", "Details", new { id=item.Makm }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Makm })
        </td>
    </tr>
}

</table>
